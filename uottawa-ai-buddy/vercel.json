{
  "version": 2,
  "cleanUrls": true,
  "rewrites": [
    { "source": "/api/stats", "destination": "/api/stats.js" },
    { "source": "/api/lb", "destination": "/api/lb.js" },
    { "source": "/dashboard", "destination": "/public/index.html" },
    { "source": "/", "destination": "/frontend/index.html" },
    { "source": "/(.*)", "destination": "/frontend/$1" }
  ]
}
